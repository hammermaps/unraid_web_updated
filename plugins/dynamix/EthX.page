<?endforeach;?>
  $('#bond-ethX').dropdownchecklist({emptyText:"_(None)_", width:<?=$width[0]?>});
  $('#bridge-ethX').dropdownchecklist({emptyText:"_(None)_", width:<?=$width[0]?>});
